public static void main(String[] args) {

   Market market = new Market();

    market.EnteredTheQueue("Олег");
    market.ordered_it("Заказ №1");
    market.update();
    market.ExitTheQueue();
    market.in_order();

    market.update();
    market.EnteredTheQueue("Иван");
    market.ordered_it("Заказ №2");
    market.update();
    market.ExitTheQueue();
    market.in_order();


    market.update();
    market.EnteredTheQueue("Катя");
    market.ordered_it("Заказ №3");
    market.update();
    market.ExitTheQueue();
    market.in_order();


}




